{
   "version": 2,
   "alias": "easy-italian-app",
   "name": "learning-italian-app",
   "routes": [
       {
         "src": "^/static/(.*)",
         "dest": "/static/$1"
       },
       {
         "src": "^/favicon.png$",
         "dest": "/favicon.png"
       },
       {
         "src": "^/manifest.json$",
         "dest": "/manifest.json"
       },
       {
         "src": "^/site.webmanifest.json$",
         "dest": "/site.webmanifest.json"
       },
       {
         "src": ".*",
         "dest": "/index.html"
       }
   ]
 }
